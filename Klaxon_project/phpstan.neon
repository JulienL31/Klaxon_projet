includes:
  - vendor/nunomaduro/larastan/extension.neon

parameters:
  level: 6
  paths:
    - app
  tmpDir: storage/framework/cache/phpstan
  checkMissingIterableValueType: false
  checkGenericClassInNonGenericObjectType: false
  larastan:
    inferModelRelations: true
  excludePaths:
    - database/*
    - bootstrap/*
    - storage/*
    - vendor/*
